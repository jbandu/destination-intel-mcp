    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "redis": "^4.7.0",
    "zod": "^3.24.1",
    "date-fns": "^4.1.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
